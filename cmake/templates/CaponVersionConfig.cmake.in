# CaponVersionConfig.cmake - 版本库配置文件
# 生成时间: @CAPON_BUILD_DATE@

# 版本信息
set(CAPON_VERSION_MAJOR @CAPON_VERSION_MAJOR@)
set(CAPON_VERSION_MINOR @CAPON_VERSION_MINOR@)
set(CAPON_VERSION_PATCH @CAPON_VERSION_PATCH@)
set(CAPON_VERSION_BUILD @CAPON_VERSION_BUILD@)
set(CAPON_VERSION_STRING "@CAPON_VERSION_MAJOR@.@CAPON_VERSION_MINOR@.@CAPON_VERSION_PATCH@.@CAPON_VERSION_BUILD@")
set(CAPON_APP_GUID "@CAPON_APP_GUID@")
set(CAPON_PRODUCT_NAME "@CAPON_PRODUCT_NAME@")
set(CAPON_COMPANY_NAME "@CAPON_COMPANY_NAME@")

# 包含目标文件
if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/CaponVersionTargets.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/CaponVersionTargets.cmake")
endif()

# 设置变量供find_package使用
set(CAPON_VERSION_FOUND TRUE)
set(CAPON_VERSION_MAJOR_VERSION @CAPON_VERSION_MAJOR@)
set(CAPON_VERSION_MINOR_VERSION @CAPON_VERSION_MINOR@)
set(CAPON_VERSION_PATCH_VERSION @CAPON_VERSION_PATCH@)
set(CAPON_VERSION_BUILD_VERSION @CAPON_VERSION_BUILD@)

# 创建导入目标别名
if(TARGET capon_version)
    add_library(Capon::Version ALIAS capon_version)
endif()